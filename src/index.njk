<section class="products-gallery padding-y-lg bg-primary">
  <div class="container max-width-adaptive-lg">
    <div class="margin-bottom-xl padding-top-xl">
      <div class="flex flex-center">
        <h1 class="color-white">Best Sellers</h1>
      </div>
    </div>

    <div class="grid gap-md">
      <div class="prod-card col-4@md">
        <span class="prod-card__badge" role="text">Best Seller <i class="sr-only">product</i></span>

        <a href="#0" class="prod-card__img-link" aria-label="Description of the link">
          <figure class="prod-card__img ">
            <img src="/assets/img/ProductFeatureImg1.jpg" alt="Photo of hand painted graduation Cap">
          </figure>
        </a>

        <div class="prod-card__content text-center">
          <h3 class="color-white"><a href="#0">Graduation Caps</a></h3>

          <div class="margin-top-xs">
            {% if sale %}
            <del class="prod-card__price prod-card__price--old color-white">$79</del> 
            {% endif %}
            <ins class="prod-card__price prod-card__price--new color-white">$49</ins>
          </div>
        </div>
      </div>
  
      <div class="prod-card col-4@md">
        <span class="prod-card__badge" role="text">New <i class="sr-only">product</i></span>

        <a href="#0" class="prod-card__img-link" aria-label="Description of the link">
          <figure class="prod-card__img media-wrapper media-wrapper--4:3">
            <img src="" alt="Product preview image">
          </figure>
        </a>

        <div class="prod-card__content text-center">
          <h3><a href="#0">Product Two</a></h3>

          <div class="margin-top-xs">
            <span class="prod-card__price">$79</span>
          </div>
        </div>
      </div>
  
      <div class="prod-card col-4@md">
        <a href="#0" class="prod-card__img-link" aria-label="Description of the link">
          <figure class="prod-card__img media-wrapper media-wrapper--4:3">
            <img src="" alt="Product preview image">
          </figure>
        </a>

        <div class="prod-card__content text-center">
          <h3><a href="#0">Product Three</a></h3>

          <div class="margin-top-xs" role="text">
            <del class="prod-card__price prod-card__price--old">$79</del> 
            <ins class="prod-card__price prod-card__price--new">$49</ins>
          </div>
        </div>
      </div>
    </div>
  
    <div class="margin-top-lg hide@md">
      <a href="#0" class="btn btn--subtle btn--md width-100%">View all products</a>
    </div>
  </div>
</section>
